import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int f=sc.nextInt();
        int s=sc.nextInt();
        if((f<0)||(s<0))
        {
        System.out.println("0");
        }
        else
        {
            System.out.println((f*4)+(s*6));
        }
        
    }
}
